<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Identifier</key>
	<string>com.github.autopkg.jleggat-recipes.download.archey-osx</string>
	<key>Description</key>
	<string>Downloads archey-osx for packaging.</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>archey-osx</string>
		<key>REPO_OWNER</key>
		<string>obihann</string>
		<key>REPO_NAME</key>
		<string>archey-osx</string>
		<key>TAG</key>
		<string>latest</string>
	</dict>
    <key>MinimumVersion</key>
    <string>0.3.0</string>
    <key>Process</key>
    <array>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>repo_owner</key>
				<string>%REPO_OWNER%</string>
				<key>repo_name</key>
				<string>%REPO_NAME%</string>
				<key>tag</key>
				<string>%TAG%</string>
			</dict>
			<key>Processor</key>
			<string>GithubTagProvider</string>
		</dict>
		<dict>
			<key>Processor</key>
			<string>URLDownloader</string>
			<key>Arguments</key>
			<dict>
				<key>filename</key>
				<string>%NAME%.zip</string>
			</dict>
		</dict>
		<dict>
			<key>Processor</key>
			<string>EndOfCheckPhase</string>
		</dict>
    </array>
</dict>
</plist>
